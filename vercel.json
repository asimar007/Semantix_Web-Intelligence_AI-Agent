{
  "functions": {
    "src/app/api/scrape/route.js": {
      "maxDuration": 60
    },
    "src/app/api/embed/route.js": {
      "maxDuration": 300
    },
    "src/app/api/chat/route.js": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
      "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome-stable"
    }
  }
}
